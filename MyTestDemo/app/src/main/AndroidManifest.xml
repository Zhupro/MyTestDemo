<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.zwy.mytestdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.test.zwy.myapp.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.test.zwy.main.MainSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ThreadTest" />
        <activity android:name="com.test.zwy.main.GuidePage"/>
        <activity android:name="com.test.zwy.main.MainActivity" />
        <activity android:name=".MyWebView" />
        <activity android:name=".MainActivityTwo" />
        <activity android:name=".HttpURlConnection" />

        <service android:name=".MyService" />
        <service android:name=".ForegtoundService" />
        <service android:name=".MyIntentService"></service>
        <service android:name=".LongRunningService"></service>

        <receiver android:name=".AlarmReceiver"></receiver>
    </application>

</manifest>